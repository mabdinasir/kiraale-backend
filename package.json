        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "tsconfig-paths": "^4.2.0",
        "validator": "^13.12.0",
        "zod": "^3.24.1"
    }
}
