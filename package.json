{
    "name": "eastleighrealestate",
    "description": "Eastleigh Real Estate",
    "module": "src/main.ts",
    "main": "src/main.ts",
    "engines": {
        "node": "22.x"
    },
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "start": "node dist/main.js || heroku restart",
        "build": "tsc && resolve-tspaths && prisma generate",
        "dev": "nodemon --exec bun run src/main.ts",
        "type-check": "tsc --noEmit --watch",
        "lint": "eslint . --ignore-pattern 'dist'",
        "lint:fix": "eslint . --fix --ignore-pattern 'dist'",
        "fmt": "prettier --write .",
        "fmt:check": "prettier --check ."
    },
    "devDependencies": {
        "@eslint/js": "^9.20.0",
        "@types/bun": "latest",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.8",
        "@types/lodash": "^4.17.15",
        "@types/validator": "^13.12.2",
        "eslint": "^9.20.0",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^15.14.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.0",
        "prisma": "^6.4.1",
        "resolve-tspaths": "^0.8.23",
        "typescript-eslint": "^8.23.0"
    },
    "peerDependencies": {
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@prisma/client": "^6.4.1",
        "axios": "^1.7.9",
